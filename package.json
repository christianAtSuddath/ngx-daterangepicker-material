{
  "name": "ngx-daterangepicker-material",
  "version": "13.0.0",
  "description": "Angular 13 date range picker (with material design theme)",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "angular10",
    "angular13",
    "ng2",
    "daterangepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "dayjs",
    "picker"
  ],
  "homepage": "https://github.com/fetrarij/ngx-daterangepicker-material",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --configuration production",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --prod --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p package.json",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cpr schematics/dist dist/schematics && cp ./schematics/collection.json ./dist/schematics && cp ./schematics/ng-add/schema.json ./dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics"
  },
  "author": "Fetrarijaona R.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fetrarij/ngx-daterangepicker-material.git"
  },
  "peerDependencies": {
    "dayjs": "^1.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.4",
    "@angular/cli": "~13.3.3",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.4",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@schematics/angular": "^13.3.3",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0",
    "core-js": "^3.22.2",
    "cpr": "^3.0.1",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "^4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "materialize-css": "^1.0.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "~7.5.0",
    "ts-node": "~10.7.0",
    "eslint": "^7.6.0",
    "typescript": "~4.6.2",
    "zone.js": "~0.11.4"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/daterangepicker/index.ts",
      "umdModuleIds": {
        "dayjs": "dayjs"
      }
    },
    "allowedNonPeerDependencies": [
      "dayjs"
    ],
    "dest": "dist"
  },
  "dependencies": {
    "dayjs": "^1.11.1",
    "tslib": "^2.3.0"
  },
  "schematics": "./schematics/collection.json"
}
